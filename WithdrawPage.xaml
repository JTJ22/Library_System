<Page x:Class="Library_System.WithdrawPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Library_System"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="WithdrawPage">

    <Grid>
        <Grid x:Name="gridWithdrawDets" HorizontalAlignment="Left" Height="430" Margin="10,0,0,0" VerticalAlignment="Center" Width="377">
            <Grid.RowDefinitions>
                <RowDefinition Height="2*" />
                <RowDefinition Height="1*" />
                <RowDefinition Height="1*" />
            </Grid.RowDefinitions>
            <TextBlock x:Name="txtBlkBookName" FontSize="16" TextWrapping="Wrap" VerticalAlignment="Top" Grid.Row="0" />
            <TextBlock x:Name="txtBlkExptReturn" FontSize="16" TextWrapping="Wrap" VerticalAlignment="Top" Grid.Row="1" Grid.Column="0" />
            <Button x:Name="btnCompleteWithdraw" Content="Withdraw" Height="41" VerticalAlignment="Top" Width="137" Click="btnCompleteWithdraw_Click" Margin="118,0,122,0" Grid.Row="2" Grid.Column="0" HorizontalAlignment="Center" />
        </Grid>
    </Grid>
</Page>
